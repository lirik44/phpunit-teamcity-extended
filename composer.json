{
    "name": "lirik44/phpunit-teamcity-extended",
    "description": "Extended support teamcity testMetadata in php STDOUT output for phpunit",
    "keywords": ["test", "teamcity", "phpunit", "php-webdriver"],
    "homepage": "https://github.com/lirik44/phpunit-teamcity-extended",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kirill Chikaliuk",
            "email": "sharapov.k44@gmail.com",
            "homepage": "https://github.com/lirik44"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "replace": {
        "phpunit/phpunit/src/Util/Log/TeamCity.php": "phpunit-teamcity-extended/src/TeamCity.php"
    },
    "autoload": {
        "psr-4": {
            "PHPUnitTeamcity\\": "src/"
        }
    }
}
